public class Main {

    public static void main(String[] args) {
        Coche miCoche = new Coche();
        miCoche.SumarPuerta();
        System.out.println(miCoche.puertas);
        }
}

    class Coche{
        public int puertas = 5;

        public void SumarPuerta() {
            this.puertas++;
        }
    }
